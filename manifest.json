{
  "manifest_version": 2,
  "name": "YouTube2Spotify",
  "description": "This looks up YouTube music videos on Spotify.",
  "version": "0.0.0.1",
  "permissions": [
    "tabs", "<all_urls>", "storage"
  ],
  "content_scripts": [
    {
      "all_frames": true,
      "matches": ["http://*/*", "https://*/*", "<all_urls>", "file:///*"],
      "js": ["jquery-2.0.0.min.js", "colourizer.js"]
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}